{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./",
    "resolveJsonModule": true,
    "paths": {
      "@@/*": [
        ".dumi/tmp/*"
      ],
      "js-tree": [
        "src"
      ],
      "js-tree/*": [
        "src/*",
        "*"
      ]
    }
  },
  "include": [
    ".dumirc.ts",
    "src/**/*",
    "types/**/*.d.ts"
  ]
}